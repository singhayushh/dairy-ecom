{
    "name": "dairy-ecom-arc",
    "version": "1.0.0",
    "description": "Developing a user-friendly dairy ecommerce platform, simplifying product exploration, ordering, and delivery for a seamless customer experience.",
    "main": "build/app.js",
    "scripts": {
        "build": "tsc -p .",
        "dev": "cross-env NODE_ENV=development nodemon ./src/app.ts",
        "docs": "typedoc --tsconfig ./tsconfig.json",
        "lint": "eslint ./src",
        "lint:fix": "eslint ./src --fix",
        "prettify": "prettier --write .",
        "start": "npm run build && cross-env NODE_ENV=production node ./build/app.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/singhayushh/holy-cow.git"
    },
    "keywords": [
        "dairy",
        "ecommerce",
        "express",
        "typescript",
        "mongoose",
        "cloudinary",
        "redis",
        "swagger",
        "authentication",
        "authorization",
        "validation",
        "logging",
        "mailer",
        "nodemon",
        "dotenv",
        "eslint",
        "prettier",
        "typescript-eslint"
    ],
    "author": "singhayushh",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/singhayushh/holy-cow/issues"
    },
    "homepage": "https://github.com/singhayushh/holy-cow#readme",
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "cloudinary": "^1.37.3",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "ejs": "^3.1.9",
        "envalid": "^8.0.0",
        "express": "^4.18.2",
        "express-validator": "^7.0.1",
        "mongoose": "^7.3.1",
        "multer": "^1.4.5-lts.1",
        "multer-storage-cloudinary": "^4.0.0",
        "nodemailer": "^6.9.3",
        "redis": "^4.6.7",
        "slug": "^8.2.3",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^4.6.3"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/multer": "^1.4.7",
        "@types/node": "^20.3.3",
        "@types/nodemailer": "^6.4.8",
        "@types/slug": "^5.0.3",
        "@types/swagger-jsdoc": "^6.0.1",
        "@types/swagger-ui-express": "^4.1.3",
        "@typescript-eslint/eslint-plugin": "^5.61.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.44.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jsdoc": "^46.4.3",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "nodemon": "^3.0.2",
        "prettier": "^2.8.8",
        "typescript": "^5.1.6",
        "winston": "^3.9.0",
        "winston-daily-rotate-file": "^4.7.1"
    }
